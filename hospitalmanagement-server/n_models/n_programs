const mongoose = require("mongoose");

const programSchema = new mongoose.Schema({
  title: {
    type: String,
    required: true,
  },

  ageGroup: {
    type: String,
    required: true,
  },

  gender: {
    type: String,
    required: true,
  },

  sample: {
    type: String,
    required: true,
  },

  desc: {
    type: String,
    required: true,
  },

  price: {
    type: String,
    required: true,
  },
});

//exporting the module
module.exports = mongoose.model("Programs", programSchema);
